{
  "name": "starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./db/schema.prisma --skip-seed",
    "db:reset": "prisma migrate reset --schema=./db/schema.prisma --skip-seed",
    "db:push": "prisma db push --schema=./db/schema.prisma --skip-seed",
    "db:seed": "npx prisma db seed",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "now-build": "yarn generate --schema=./db/schema.prisma && next build"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.13.0",
    "@tailwindcss/forms": "^0.5.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "^12.1.6",
    "next-auth": "4.3.4",
    "next-connect": "^0.12.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "react-query": "^3.38.1",
    "superagent": "^7.1.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} db/seed.ts"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/superagent": "^4.1.15",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0"
  }
}
